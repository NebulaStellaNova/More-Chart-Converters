<?xml version="1.0" encoding="utf-8"?>
<dialog id="chartDialog" width="600" height="400" title="From Codename Engine - Upload Instrumental">
	<vbox width="100%" height="100%">
		<box width="100%" id="instBox" height="100%" styleName="upload-bg" style="border: 2px dotted #DDDDDD; padding: 5px;">
			<label id="theLabel" width="100%" style="font-size: 24px" verticalAlign="center" horizontalAlign="center" textAlign="center" text="Click here to browse for an Instrumental file (.ogg)" />
		</box>
		<hbox>
			<button text="Cancel" id="dialogCancel" />
			<button text="Skip (No Instrumental)" id="dialogSkip"/>
			<spacer width="100%" />
		</hbox>
	</vbox>
</dialog>
